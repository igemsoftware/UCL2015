{
  "name": "MarkGEM",
  "description": "Turns the basic wiki text input into a real code editor",
  "manifest_version": 2,
  "version" : "0.2",
  "icons":{ "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "content_scripts":          [ {
    "matches":  [ "<all_urls>"],
	"js":       ["jquery-1.11.3.min.js","background.js"]
	
	} ],
  "web_accessible_resources": ["script.js"]
}
